{
  "name": "@flatfile/listener-driver-pubsub",
  "version": "1.0.8",
  "description": "A driver for streaming events from Flatfile via a pubsub pattern",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts  --watch",
    "test": "jest --passWithNoTests",
    "prepublish": "npm run build"
  },
  "files": [
    "dist/**"
  ],
  "author": "David Boskovic",
  "license": "ISC",
  "devDependencies": {
    "@flatfile/eslint-config-platform-sdk": "*",
    "@flatfile/jest-preset-platform-sdk": "*",
    "@flatfile/ts-config-platform-sdk": "*",
    "@types/jest": "^29.5.1",
    "eslint": "^8.19.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.4.1",
    "tsup": "^6.1.3",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@flatfile/listener": "^0.3.15",
    "@flatfile/api": "^1.5.5",
    "axios": "^1.4.0",
    "@flatfile/utils-debugger": "^0.0.5",
    "pubnub": "^7.2.2"
  },
  "peerDependencies": {
    "@flatfile/listener": "0.3.15"
  },
  "overrides": {
    "pubnub@^7.2.2": {
      "superagent-proxy@^3.0.0": {
        "proxy-agent@^5.0.0": {
          "pac-proxy-agent@^5.0.0": {
            "pac-resolver@^5.0.0": {
              "degenerator": "^5.0.0"
            }
          }
        }
      }
    }
  }
}